<div class="section no-pad-bot">
  <div class="container">
    <br><br>
    <h1 class="header center white-text hero-h1">SpacePort(folio)</h1>
    <div class="row center">
      <h5 class="header col s12 header center white-text hero-h2">A responsive image search app using Materialize</h5>
    </div>
    <div class="row center">
      <!--       <a href="#" id="portfolio-button" class="btn-large waves-effect waves-light teal lighten-1">Create Portfolio</a>
--></div>
    <br><br>

  </div>

</div>
<div class="parallax"><img src="/assets/img/bg40.png" alt="Hero BG IMG"></div>
</div>


<div class="row" id="row-1">
  <div class="parallax"><img src="/assets/img/bg40.png" alt="Hero BG IMG"></div>

  {{#each photos}}
  <div class="col s12 m3">
    <div class="card">
      <div class="card-image waves-effect waves-block waves-light">
        <img class="activator" src={{this.url}} >
      </div>
      <div class="card-content">
        <span class="card-title activator grey-text text-darken-4">{{this.title}}<i class="material-icons right">more_vert</i></span>
        <p>Add To Portfolio</p>
        <p>
          <!-- Switch -->
          <div class="switch">
            <label>
            Off
            <input type="checkbox">
            <span class="lever"></span>
            On
          </label>
          </div>
        </p>
      </div>

      <div class="card-reveal">
        <span class="card-title grey-text text-darken-4">{{this.title}}<i class="material-icons right">close</i></span>
        <p>{{this.explanation}}</p>
      </div>
    </div>
  </div>
  {{/each}}
</div>
</div>
