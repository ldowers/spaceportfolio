<div class="row" id="form-overlay">
  <div class="parallax"><img src="/assets/img/bg40.png" alt="Hero BG IMG"></div>
  <button class="btn waves-effect waves-light serif blue darken-4 center-align" id="form-close-btn"><a href="#">&times;</a></button>
  <div class="col m3"></div>
  <form class="col s12 m6">
    <div class="row">
      <h4 class="center-align top-margin white-text form-h4">Please login or create your Space Portfolio</h4><br><br>

    </div>
    <div class="row">
      <div class="input-field col s12">
        <form class="login" id="form" action="/spaceportfolio/signup">
        <div class="form-group">
        <input id="email-input" type="email" class="validate">
        <label for="email" class="white-text sans">Email</label>
        </div>
      </div>
      <div class="input-field col s12">
        <div class="form-group">
        <input id="password-input" type="password" class="validate">
        <label for="password" class="white-text sans">Password</label>
        </div>
        <button class="btn waves-effect waves-light serif blue darken-3" type="button" name="action" id="form-submit-btn">Submit
          <!-- <i class="material-icons right">send</i>
        -->        </button>
        </form>
      </div>

    </div>

  </form>

</div>




